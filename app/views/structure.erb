<html>
  <head>
    <title>YummySoup Viewer</title>
    <link href='http://fonts.googleapis.com/css?family=Oxygen+Mono' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" type="text/css" media="only screen and (max-device-width: 1024px)" href="css/kindle_fire.css" />
    <link href="../css/bootstrap.min.css" rel="stylesheet" media="screen">
  </head>
  <body>
  <%= erb :header %>
<ul>
  <% @recipe.each { |x, y| %>
    <li>
      <%= "Key: <strong>" + x.to_s + "</strong>" %>
    <br>
      <%= "Value: " + y.to_s %>
    </li>
  <% } %>
  </body>
</html>
